      <input type="radio" name="performance" value="<%= qs.sound4 %>">performance or live experimentation<br>
      <input type="radio" name="musicconcrete" value="<%= qs.sound5 %>">music concrète<br>
      <input type="radio" name="podcast" value="<%= qs.sound6 %>">podcast or talk show<br>
      <input type="radio" name="other" value="<%= qs.sound7 %>">other<br>
      </fieldset>
